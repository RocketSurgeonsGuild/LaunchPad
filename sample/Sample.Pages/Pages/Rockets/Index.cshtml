@page
@using Microsoft.AspNetCore.Mvc.TagHelpers
@model RocketIndexModel

<a class="card-link" asp-page="Create">Create</a>
<table class="table">
    <thead>
    <tr>
        <th scope="col">Serial Number</th>
        <th scope="col">Type</th>
        <th scope="col"></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var rocket in Model.Rockets)
    {
        <tr>
            <th scope="row">
                <a asp-page="View" asp-route-id="@rocket.Id">@rocket.Sn</a>
            </th>
            <td>@rocket.Type</td>
            <td></td>
        </tr>
    }
    </tbody>
</table>